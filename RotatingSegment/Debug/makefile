# Add inputs and outputs from these tool invocations to the build variables 
LSS += \
RotatingSegment.lss \

FLASH_IMAGE += \
RotatingSegment.hex \

SIZEDUMMY += \
sizedummy \


# All Target
all: RotatingSegment.elf secondary-outputs

# Tool invocations
RotatingSegment.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: AVR C Linker'
	avr-gcc -Wl,-Map,RotatingSegment.map -mmcu=atmega32 -o "RotatingSegment.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

RotatingSegment.lss: RotatingSegment.elf
	@echo 'Invoking: AVR Create Extended Listing'
	-avr-objdump -h -S RotatingSegment.elf  >"RotatingSegment.lss"
	@echo 'Finished building: $@'
	@echo ' '

RotatingSegment.hex: RotatingSegment.elf
	@echo 'Create Flash image (ihex format)'
	-avr-objcopy -R .eeprom -R .fuse -R .lock -R .signature -O ihex RotatingSegment.elf  "RotatingSegment.hex"
	@echo 'Finished building: $@'
	@echo ' '

sizedummy: RotatingSegment.elf
	@echo 'Invoking: Print Size'
	-avr-size --format=avr --mcu=atmega32 RotatingSegment.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(FLASH_IMAGE)$(ELFS)$(OBJS)$(ASM_DEPS)$(S_DEPS)$(SIZEDUMMY)$(S_UPPER_DEPS)$(LSS)$(C_DEPS) RotatingSegment.elf
	-@echo ' '

secondary-outputs: $(LSS) $(FLASH_IMAGE) $(SIZEDUMMY)

.PHONY: all clean dependents
